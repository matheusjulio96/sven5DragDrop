@inherits ComponentBase

@using sven5DragDrop.Services

<div class="alert alert-primary mt-4" role="alert">
    <strong>@Model.Text</strong>
    <span class="text-nowrap">
        @Model.Description (Index: @Model.Index)
    </span>
</div>

@code {
    [Parameter]
    DragItem Model { get; set; }
}
